@model document_anal.Models.DocumentViewErrorModel
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Error</title>
</head>
<body>
    <hgroup>
        <h1>Error.</h1>
        <h2>Source: <br>@Html.Raw(Model.Position)</h2>
        <h2>Description: <br>@Html.Raw(Model.Description)</h2>
        <h2>StackTrace <br>@Html.Raw(Model.StackTrace)</h2>

    </hgroup>
</body>
</html>
